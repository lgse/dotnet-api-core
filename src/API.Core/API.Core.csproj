<Project Sdk="Microsoft.NET.Sdk.Web">
    <PropertyGroup>
        <TargetFramework>net6.0</TargetFramework>
        <AssemblyName>LGSE.API.Core</AssemblyName>
        <PackageId>LGSE.API.Core</PackageId>
        <Title>Dotnet Core Components to build RESTful APIs</Title>
        <Authors>LGSE Ltd.</Authors>
        <PackageProjectUrl>https://github.com/lgse/dotnet-api-core</PackageProjectUrl>
        <RepositoryUrl>https://github.com/lgse/dotnet-api-core</RepositoryUrl>
        <RepositoryType>git</RepositoryType>
        <PackageTags>dotnet,api,core</PackageTags>
        <Company>LGSE Ltd.</Company>
    </PropertyGroup>
    <ItemGroup>
        <PackageReference Include="AutoMapper" Version="12.0.0" />
        <PackageReference Include="FluentValidation" Version="11.2.2" />
        <PackageReference Include="Microsoft.EntityFrameworkCore" Version="6.0.9" />
        <PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
        <PackageReference Include="Paramore.Brighter" Version="9.2.2" />
        <PackageReference Include="StackExchange.Redis" Version="2.6.66" />
    </ItemGroup>
</Project>
